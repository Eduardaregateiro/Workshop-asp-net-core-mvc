@model SalesWebMvc.Models.Seller
@{
    ViewData["Title"] = "Create";
}
<h3>@ViewData["Title"]</h3>

<h5>Seller</h5>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>"
                <input asp-for="Name" class="for-control"/>"
            </div>
            <div class="form-group">
                <label asp-for="Email" class="control-label"></label>"
                <input asp-for="Email" class="for-control"/>"
            </div>
            <div class="form-group">
                <label asp-for="BirthDate" class="control-label"></label>"
                <input asp-for="BirthDate" class="for-control" />"
            </div>
            <div class="form-group">
                <label asp-for="BaseSalary" class="control-label"></label>"
                <input asp-for="BaseSalary" class="for-control" />"
            </div>
            <div class="form-group">
                <input type="submit" value=" Create" class=" btn btn-defoult"
            </div>
        </form>
    </div>
</div>